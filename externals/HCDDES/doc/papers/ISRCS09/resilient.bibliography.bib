@ARTICLE{kottenstette08:_contr_of_multip_networ_passiv,
title={Control of multiple networked passive plants with delays and data dropouts},
author={Kottenstette, N. and Antsaklis, P.J.},
journal={American Control Conference, 2008},
year=2008,
month={June},
pages={3126-3132},
abstract={This paper provides a framework to synthesize l2 m-stable and L2 m-stable control networks in which m strictly-output passive controllers can control n - m strictly-output passive plants. The communication between the plants and controllers can tolerate time varying delay and data dropouts. In particular, we introduce a power junction which allows even a single controller (typically designed to control a single plant) to accurately control the position of multiple plants even if the dynamics of the plants are different. An illustrative simulated example shows the position tracking performance of the system. We conclude the discussion with two questions for future research.},
keywords={control system synthesis, delays, time-varying systemscontrol design, data dropouts, multiple networked passive plants control, position tracking performance, power junction, time varying delay},
