@Article{ji90:_jump_gauss,
language = {English},
copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
title = {Jump linear quadratic Gaussian control. Steady-state solution and testable conditions},
journal = {Control - Theory and Advanced Technology},
author = {Ji, Y. and Chizeck, H.J.},
volume = { 6},
number = { 3},
year = 1990,
pages = {289 - 319},
issn = {0911-0704},
abstract = {This paper is concerned with the control of discrete-time linear systems that posses randomly jumping parameters which can be described by an underlying finite-state Markov chain. It is assumed there are additive Gaussian disturbances to the system input and output. This model can be used to formulate systems that are subject to abrupt changes in their structure and components, due to failures, repairs or changing subsystem interconnections. This class of model can also be used to represent nonlinear systems, jumping between different linearized models as the operating point changes. Because part of the state takes values in a real vector space while the jumping parameter (which is also part of the system state) can only take discrete values, this type of system is sometimes called a 'hybrid system'. The control of hybrid systems has potential applications involving systems requiring fault-tolerant control. Although discrete time Jump Linear Quadratic (JLQ) optimal control problems, with and without Gaussian noise, have been solved in the literature, conditions available to test the existence of steady-state solutions are, unfortunately, very difficult to implement. In this paper, new necessary and sufficient conditions are established in a form that is directly testable, using recursive computations. These new conditions are based upon stochastic stabilizability and detectability properties for discrete time jump linear systems. The importance of this paper is that it makes feasible the use of jump linear quadratic Gaussian control theory for controller design in applications.},
key = {Control Systems, Discrete Time -- Mathematical Models},
keywords = {Control Systems, Linear--Mathematical Models;},
note_1 = {Jump Linear Quadratic (JLQ) Gaussian Control;Finite State Markov Chain;Jumping Parameter;Hybrid Systems;Testable Conditions;Stochastic Stabilizability/Detectability;},
} 
