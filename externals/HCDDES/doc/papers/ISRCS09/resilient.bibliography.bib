@Article{seiler05:_h,
language = {English},
copyright = {Compilation and indexing terms, Copyright 2006 Elsevier Inc. All rights reserved},
title = {An H infinity approach to networked control},
journal = {IEEE Transactions on Automatic Control},
author = {Seiler, Pete and Sengupta, Raja},
volume = { 50},
number = { 3},
year = 2005,
pages = {356 - 364},
issn = {0018-9286},
abstract = {In this paper, we study the effect of a network in the feedback loop of a control system. We use a stochastic packet-loss model for the network and note that results for discrete-time linear systems with Markovian jumping parameters can be applied. We measure performance using an H<sub> infinity </sub> norm and compute this norm via a necessary and sufficient matrix inequality condition. We also derive necessary and sufficient linear matrix inequality (LMI) conditions for the synthesis of the H<sub> infinity </sub> optimal controller for a discrete-time jump system. Finally, we apply these results to study the effect of communication losses on vehicle control. &copy; 2005 IEEE.},
