  atomic type CallingUnit(int px, int py)
    data int x=px
    data int y=py
    data int z=0
    export port RequestPort request(x, y)
    export port EnterPort enter(x, y)
    place OCCUPIED
    place EMPTY
    initial to EMPTY
do {
      }
    on request
      from EMPTY to OCCUPIED
      provided z<=200
      do z = z+1;

    on enter
      from OCCUPIED to EMPTY


  end

