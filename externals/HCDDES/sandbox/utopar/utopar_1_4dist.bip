model utopar_1_4
  {# 
 
   int delta(int u, int du) { 
     return (u < du) ? 1 : (u > du) ? -1 : 0; 
   }
 
  #}
  header {#

#include <assert.h>

// warning: matrices are used from 1 to M

const int Z = 20;

typedef int matrix[Z][Z];
#}
  {# 

void initializeMatrix(matrix& m, const int v = 0) {
        for(int i = 0; i < Z; i++)
                for(int j = 0; j < Z; j++)
                        m[i][j] = v;
}

void copyMatrix(matrix& m1, const matrix& m2) {
    for(int i = 0; i < Z; i++)
      for(int j = 0; j < Z; j++)
        m1[i][j] = m2[i][j];
}

#}
